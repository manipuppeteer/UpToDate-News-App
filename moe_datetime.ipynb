   "source": [
    "import datetime\n",
    "jetzt = datetime.datetime.now()\n",
    "time = '2025-08-12T08:28:03Z'\n",
    "zeit_datetime = datetime.datetime.strptime(time, '%Y-%m-%dT%H:%M:%SZ')\n",
    "\n",
    "diff = jetzt - zeit_datetime\n",
    "seconds = diff.seconds\n",
    "\n",
    "hours = seconds//3600\n",
    "minutes = (seconds - hours*3600)//60\n",
    "hours, minutes"
   ]
  }
 ],
